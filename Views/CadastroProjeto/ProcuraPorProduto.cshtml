@using SEDOGv2.Models;
@{
    ViewBag.Title = "ProcuraPorProduto";
    //Layout = "~/Views/Shared/_LoggedIn.cshtml";
}
<div id="mainContent">
    <div class="input-group">
        <input type="text" class="form-control input-md" id="pesquisaProduto" placeholder="Product" />
        <span class="input-group-btn">
            <button class="btn btn-default btn-md" type="button" id="btnProcuraProdutos"><i class="fa fa-search"></i></button>
        </span>
    </div>
    <p>&nbsp;</p>
    <div id="tblProdutos">
        <table class="table table-striped table-bordered table-inline tableSortPaggingDivPrd" cellspacing="0" width="100%">
            <thead>
                <tr class="bg-default">
                    <th>Product Code</th>
                    <th>Packing</th>
                    <th>Type</th>
                    <th>CIA</th>
                    <th>Artist</th>
                    <th>Title</th>
                    <th>Release Date</th>
                    <th>Cordis Date</th>
                    <th>BU</th>
                </tr>
            </thead>
        </table>
    </div>
</div>
